<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters="using:Lifethreadening.converters">
    <SolidColorBrush x:Key="PrimaryBackgroundBrush" Color="#A1A1A1" />
    <LinearGradientBrush x:Key="SecondaryBackgroundBrush" StartPoint="0,0.5" EndPoint="1,0.5">
        <GradientStop Color="#000000" />
        <GradientStop Color="#424242" />
    </LinearGradientBrush>
    <SolidColorBrush x:Key="ternaryBackgroundBrush" Color="#D9D9D9"></SolidColorBrush>
    <SolidColorBrush x:Key="PrimaryBrush" Color="Black" />
    <SolidColorBrush x:Key="SecondaryBrush" Color="White" />
    <x:Double x:Key="FontsizeGenetics" >18</x:Double>
    <converters:IntFormatConverter x:Key="intFormat"></converters:IntFormatConverter>
</ResourceDictionary>
